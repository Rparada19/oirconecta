{
  "name": "oirconecta-monorepo",
  "private": true,
  "description": "OirConecta - Frontend y Backend",
  "scripts": {
    "dev": "concurrently -n backend,frontend -c blue,green \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd oirconecta && npm run dev"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
